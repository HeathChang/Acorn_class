package com.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import com.dao.DeptOracleDAO;

public class DeptService {
	@Autowired
	public DeptOracleDAO dao;

	public DeptService(DeptOracleDAO dao) {
		System.out.println("DeptService DAO 생성자 호출됨");
		this.dao = dao;
	}

	public DeptOracleDAO getDao() {
		return dao;
	}

	public DeptService() {
		super();
		// TODO Auto-generated constructor stub
	}

	public void setDao(DeptOracleDAO dao) {
		System.out.println("DeptService DAO set함수 호출됨");
		this.dao = dao;
	}

	public List<String> getList() {
		return dao.getList();
	}

}
